@model string
@{
    var normalized = (Model ?? string.Empty).Trim().ToLowerInvariant();
    var cssClass = normalized switch
    {
        "high" or "healthy" => "bg-success",
        "medium" or "warning" => "bg-warning text-dark",
        "low" or "risk" => "bg-danger",
        _ => "bg-primary"
    };
}
<span class="badge @cssClass">@Model</span>
